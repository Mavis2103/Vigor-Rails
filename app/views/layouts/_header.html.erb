  <section class="header_Nav">
    <header class="home__header">
      <div class="home__header__logo">
        <%= link_to 'VIGOR', '/v1/home', class: 'home__header__logo'%>
      </div>
      <nav class="home__header__link">
        <a href="">Trang Chủ</a>
        <a href="">Cộng đồng</a>
        <a href="">LIVE</a>
        <a href="">FAQs</a>
        <div class="search"></div>
      </nav>
      <nav class="home__header__button">
        <% if !@user.nil? %>
        <a href="/v1/profile"><%= @user_session[0].username %></a>
        <% p request.original_url != "http://localhost:3000/v1/profile" %>
        <% if ((request.original_url != "http://localhost:3000/v1/profile"))%>
        <a id="home__header__button--logout">Đăng xuất</a>
        <% else %>
        <a href="http://localhost:3000/v1/home">Về trang chủ</a>
        <% end %>
      </nav>
      <% end %>
    </header>
  </section>