  <section class="header_Nav">
    <header class="home__header">
      <div class="home__header__logo">
        <%= link_to 'VIGOR', '/v1/home', class: 'home__header__logo'%>
      </div>
      <nav class="home__header__link">
        <a href="">Trang Chủ</a>
        <a href="">Cộng đồng</a>
        <a href="">LIVE</a>
        <a href="">FAQs</a>
        <div class="search"></div>
      </nav>
      <% if !@user.nil? %>
      <nav class="home__header__button">
        <a href="/v1/profile"><%= @user[0].username %></a>
        <button id="home__header__button--logout">Đăng xuất</button>
      </nav>
      <% else %>
      <nav class="home__header__button">
        <a href="/v1/login">Đăng nhập</a>
        <a href="/v1/signup">Đăng ký</a>
      </nav>
      <% end %>
    </header>
  </section>